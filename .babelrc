{
  "presets": [
    "babel-preset-kyt-react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "./tools/babelRelayPlugin.js",
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-class-properties",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components": "./components",
        "routes": "./routes",
        "utils": "./utils"
      }
    }]
  ]
}
