{
  "presets": [
    "babel-preset-kyt-react",
    "stage-0"
  ],
  "plugins": [
    ["relay", {"compat": true, "schema": "./tools/schema.graphql"}],
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-class-properties",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "routes": "./src/routes",
        "components": "./src/components",
        "routes": "./src/routes",
        "utils": "./src/utils"
      }
    }]
  ]
}
